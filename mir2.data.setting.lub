setting.initEnd = function ()
	--- BLOCK #0 1-18, warpins: 1 ---
	sound.setEnable(setting.base.soundEnable)

	an.label.normal = setting.cpu.normalFont
	local filt = setting.item.filt
	--- END OF BLOCK #0 ---

	slot1 = if not filt.极品属性道具 then
	JUMP TO BLOCK #1
	else
	JUMP TO BLOCK #2
	end


	--- BLOCK #1 19-20, warpins: 1 ---
	filt.极品属性道具 = {
		hintName = true,
		pickUp = true,
		isGood = true,
		pickOnRatting = true
	}

	--- END OF BLOCK #1 ---

	FLOW; TARGET BLOCK #2


	--- BLOCK #2 21-30, warpins: 2 ---
	setmetatable(filt, {
		__index = _G.def.items.filt
	})

	return 
	--- END OF BLOCK #2 ---



end
