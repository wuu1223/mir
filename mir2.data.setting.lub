end
local default = clone(setting)
setting.reset = function ()
	--- BLOCK #0 1-4, warpins: 1 ---
	--- END OF BLOCK #0 ---

	for k, v in pairs(default)


	LOOP BLOCK #1
	GO OUT TO BLOCK #3


	--- BLOCK #1 5-9, warpins: 1 ---
	setting[k] = clone(v)

	--- END OF BLOCK #1 ---

	FLOW; TARGET BLOCK #2


	--- BLOCK #2 10-11, warpins: 2 ---
	--- END OF BLOCK #2 ---

	UNCONDITIONAL JUMP; TARGET BLOCK #0


	--- BLOCK #3 12-15, warpins: 1 ---
	setting.initEnd()

	return 
	--- END OF BLOCK #3 ---



end
setting.init = function (playerName)
	--- BLOCK #0 1-7, warpins: 1 ---
	setting.reset()
	--- END OF BLOCK #0 ---

	for k, v in pairs(setting)


	LOOP BLOCK #1
	GO OUT TO BLOCK #6


	--- BLOCK #1 8-14, warpins: 1 ---
	local saved = cache.getSetting(playerName, k)

	--- END OF BLOCK #1 ---

	slot6 = if saved then
	JUMP TO BLOCK #2
	else
	JUMP TO BLOCK #5
	end


	--- BLOCK #2 15-18, warpins: 1 ---
	--- END OF BLOCK #2 ---

	for k2, v2 in pairs(saved)


	LOOP BLOCK #3
	GO OUT TO BLOCK #5


	--- BLOCK #3 19-19, warpins: 1 ---
	v[k2] = v2

	--- END OF BLOCK #3 ---

	FLOW; TARGET BLOCK #4


	--- BLOCK #4 20-21, warpins: 2 ---
	--- END OF BLOCK #4 ---

	UNCONDITIONAL JUMP; TARGET BLOCK #2


	--- BLOCK #5 22-23, warpins: 3 ---
	--- END OF BLOCK #5 ---

	UNCONDITIONAL JUMP; TARGET BLOCK #0


	--- BLOCK #6 24-27, warpins: 1 ---
	setting.initEnd()

	return 
	--- END OF BLOCK #6 ---



end

return setting
